{
  "name": "inelegis",
  "version": "0.0.7",
  "description": "Sistema de Consulta de Inelegibilidade Eleitoral",
  "main": "index.html",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "node scripts/lint.js",
    "lint:fix": "node scripts/lint.js --fix",
    "test": "node scripts/test.js",
    "test:unit": "node tests/formatters.test.js && node tests/exceptions.test.js",
    "test:all": "npm run test:unit && npm run test",
    "validate": "node scripts/verify-data.js",
    "backup": "node scripts/backup-data.js create",
    "serve": "node scripts/serve.js",
    "dev": "npm run serve",
    "check": "npm run lint && npm run validate && npm run test",
    "optimize": "node scripts/optimize.js",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "NODE_ENV=production npm run deploy",
    "migrate": "node scripts/migrate.js",
    "rollback": "node scripts/rollback.js",
    "generate-token": "node scripts/generate-admin-token.js",
    "start": "npm run serve",
    "prebuild": "npm run check",
    "preoptimize": "npm run build",
    "predeploy": "npm run optimize",
    "clean": "rimraf dist dist-optimized deploy",
    "clean:all": "npm run clean && rimraf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "update-deps": "npm update && npm audit fix",
    "size": "node -e \"const fs=require('fs'); const path=require('path'); const getSize=d=>fs.readdirSync(d).reduce((s,f)=>{const p=path.join(d,f); return s+(fs.statSync(p).isDirectory()?getSize(p):fs.statSync(p).size)},0); console.log('Tamanho do projeto:', Math.round(getSize('.')/1024)+'KB');\""
  },
  "keywords": [
    "inelegibilidade",
    "sistema-juridico"
  ],
  "author": "Inelegis",
  "license": "MIT",
  "dependencies": {
    "@vercel/kv": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "html-validate": "^8.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkvasne/ineleg-app.git"
  },
  "bugs": {
    "url": "https://github.com/rkvasne/ineleg-app/issues"
  },
  "homepage": "https://github.com/rkvasne/ineleg-app#readme"
}