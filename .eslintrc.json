{
  "env": { "browser": true, "es2021": true },
  "extends": ["eslint:recommended"],
  "parserOptions": { "ecmaVersion": 2021, "sourceType": "script" },
  "globals": {
    "leisDisponiveis": "readonly",
    "tabelaInelegibilidade": "readonly"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "no-redeclare": "off",
    "no-extra-semi": "off",
    "no-useless-escape": "off",
    "no-empty": "off"
  }
}
